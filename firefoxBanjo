#!/bin/bash

# about:profiles
# /Applications/Firefox.app/Contents/MacOS/firefox-bin -P
# https://www.ghacks.net/2017/10/08/the-most-important-firefox-command-line-options/
# /Applications/Firefox.app/Contents/MacOS/firefox-bin -P "default" --browser &

# http://sonnygill.net/mac/mac-multiple-firefox-profiles/
/Applications/Firefox.app/Contents/MacOS/firefox-bin -P "default" -no-remote &> /dev/null &
